{% autoescape true %}
{
"match":
	{
	"date":"{{match.date}}",
	"field":
		{
			"name":"{{field.name}}",
			"location":"{{field.location}}"
		},
	"home":"{{match.home}}",
	"away":"{{match.away}}",
	"referees":[
			{% for ref in referees %}
				{
				"name":"{{ref.name}}"
				}
			{% if not loop.last %},{% endif %}
			{% endfor %}
		]
	}
}
{% endautoescape %}
