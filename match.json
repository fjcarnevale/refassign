{% autoescape true %}
{
"match":
	{
	"date":"{{match.date}}",
	"field":
		{
			"name":"{{field.name}}",
			"location":"{{field.location}}"
		},
	"teams":[
			{% for team in teams %}
				{
				"name":"{{team.name}}"
				}
			{% if not loop.last %},{% endif %}
			{% endfor %}
		],
	"referees":[
			{% for ref in referees %}
				{
				"name":"{{ref.name}}",
				"grade":{{ref.grade}}
				}
			{% if not loop.last %},{% endif %}
			{% endfor %}
		]
	}
}
{% endautoescape %}
